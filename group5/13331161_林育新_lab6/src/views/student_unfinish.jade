extends basestudent

block content-right
    .unfinish
      .page-header
        h1
          | 软件过程改进
      table.table.table-bordered
        tr
          th.col-md-2 作业名
          th.col-md-2 发布者
          th.col-md-3 要求
          th.col-md-3 截止日期
          th.col-md-2 操作
        each course in hw
            tr
              td.changeStuUn #{course.hwname}
              td.changeStuUn #{course.createby}
              td.changeStuUn #{course.command}
              td.changeStuUn.highlight #{course.ddl}
              td
                a.btn.btn-primary(role="button" href="/handin/#{course._id}") 提交
                a.btn.btn-primary.changeUnfinishbutton(role="button") 查看